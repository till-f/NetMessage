<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>NetMessage</RootNamespace>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId></PackageId>
    <AssemblyName>NetMessage</AssemblyName>
    <Authors>Till Fischer</Authors>
    <Description>Typesafe, lightweight RPC/messaging for .NET</Description>
    <Copyright>(c) 2023</Copyright>
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <PackageProjectUrl>https://github.com/till-f/NetMessage</PackageProjectUrl>
    <RepositoryUrl>https://github.com/till-f/NetMessage.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>RPC;Networking;Messaging</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\NetMessage.Base\NetMessage.Base.csproj" />
  </ItemGroup>

</Project>
